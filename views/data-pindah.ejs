    <div class="row g-0 mt-5">
      <!-- header start -->
      <%- include('sidebar'); -%>
      <!-- header end   -->
      <div class="col-md-10 p-5 pt-5">
        <h3><i class="fa-solid fa-cloud-arrow-down"></i>&nbsp&nbspDATA PINDAH</h3><hr>
        <a href="/tambah_data/tambah-data-pindah" class="btn btn-primary mb-3 "><i class="fa-solid fa-pen-to-square"></i>Tambah Data</a>
        <!-- First Row -->
        <table id="dtHorizontalVerticalExample" class="table table-striped table-bordered table-sm " cellspacing="0"
          width="100%">
          <thead>
            <tr>
              <th scope="col" class="text-center">No</th>
              <!-- <th scope="col" class="text-center">Id Pindah</th> -->
              <th scope="col" class="text-center">Nomor KK</th>
              <th scope="col" class="text-center">Tanggal Pindah</th>
              <th scope="col" class="text-center">NIK</th>
              <th scope="col" class="text-center">Alasan</th>
              <th scope="col" class="text-center">Aksi</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td scope="row" class="text-center">1</td>
              <!-- <td scope="row" class="text-center">Tiger</td> -->
              <td scope="row" class="text-center">1234567891234567</td>
              <td scope="row" class="text-center">11/08/2022</td>
              <td scope="row" class="text-center">1234567891234567</td>
              <td scope="row" class="text-center">Bekerja</td>
              <td scope="row" class="text-center">
                <button class="border-0" ><i class="fa-solid fa-users-rectangle bg-primary p-2 text-white rounded" data-toggle="tooltip" title="Detail"></i></button>
                <button class="border-0" type="button" data-bs-toggle="modal" data-bs-target="#exampleModalEdit"><i class="fa-solid fa-pen-to-square bg-success p-2 text-white rounded" data-toggle="tooltip" title="Edit"></i></button>
                <button class="border-0"><i class="fa-solid fa-trash-can bg-danger p-2 text-white rounded" data-toggle="tooltip" title="Delete"></i></button>
              </td>
            </tr>
          </tbody>
        </table>
      </div>

      <!-- Modal Edit -->
  <div class="modal fade" id="exampleModalEdit" tabindex="-1" aria-labelledby="exampleModalLabel"
  aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabelEdit">Edit Data Penduduk</h5>
        <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body table-responsive">
        <table class="table table-bordered no-margin">
          <tbody>
            <tr>
              <th style="width:50%">No. KK</th>
              <td>5371041506020007</td>
            </tr>
            <tr>
              <th style="width:50%">NIK</th>
              <td>5371041506020007</td>
            </tr>
              <th style="width:50%">Tanggal Pindah</th>
              <td>19/08/2012</td>
            </tr>
            <tr>
              <th style="width:50%">Alasan Pindah</th>
              <td>Dapatr Suanggi</td>
            </tr>
          </tbody>
        </table>
      </div>

      <div class="modal-footer">

        <button type="button" class="btn btn-primary">Simpan</button>
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>


      <!-- footer start -->
      <%- include('footer'); -%>
      <!-- footer end   -->
      <script>
        $(document).ready(function () {
        $('#dtHorizontalVerticalExample').DataTable({
          "scrollX": true,
          "scrollY": 200,
        });
        $('.dataTables_length').addClass('bs-select');
        });

      </script>
    
